CC=gcc
CFLAGS=-I.

hellomake: structs.c 
	$(CC) -o structs structs.c
	$(CC) -o typedef typedef.c
	$(CC) -o typedef_challange typedef_challange.c
	$(CC) -o struct_pointer_sample struct_pointer_sample.c
	$(CC) -o struct_pointer struct_pointer.c
	$(CC) -o student student.c
	$(CC) -o student_pointer student_pointer.c
